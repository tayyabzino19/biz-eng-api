{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "post"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "post_title": {
      "type": "string"
    },
    "location": {
      "type": "string"
    },
    "lat": {
      "type": "float"
    },
    "lng": {
      "type": "float"
    },
    "media": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "users_permissions_user": {
      "type": "relation",
