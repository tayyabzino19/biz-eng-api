      "target": "plugin::users-permissions.user",
      "inversedBy": "posts"
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "post"
    },
    "post_dislike": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::post-dislike.post-dislike",
      "inversedBy": "post"
    },
    "post_like": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::post-like.post-like",
      "mappedBy": "post"
    },
    "report_post": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::report-post.report-post",
      "inversedBy": "post"
    }
